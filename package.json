	"repository": {
		"type": "git",
		"url": "https://github.com/your-org/interview-app.git"
	},
	"scripts": {
		"analyze": "tsx src/lib/structure-analysis/cli/analyze.ts",
		"worker:start": "NODE_ENV=production tsx --env-file=.env.local src/infrastructure/bullmq/worker.ts",
		"worker:dev": "tsx --env-file=.env.local src/infrastructure/bullmq/worker.ts",
		"dev:all": "concurrently \"pnpm dev\" \"pnpm worker:dev\" --names \"app,worker\" --prefix-colors \"cyan,magenta\"",
		"test:coverage": "tsx scripts/coverage-check.ts",
		"test:sla": "tsx cli/evaluate-harness.ts",
		"build:worker": "node scripts/build-worker.mjs",
		"build:all": "concurrently \"pnpm build\" \"pnpm build:worker\" --names \"app,worker\" --prefix-colors \"cyan,magenta\"",
		"build": "next build",
		"check:all": "biome check . && tsc --noEmit",
		"clean": "rm -rf .next dist build coverage test-results",
		"clean:all": "rm -rf node_modules .next dist build coverage test-results && pnpm install",
		"dev": "next dev --turbopack --port 3000",
		"fix:all": "biome check . --write --max-diagnostics 50 --diagnostic-level error && tsc --noEmit",
		"format": "biome format .",
		"format:fix": "biome format --write .",
		"install-deps": "bash .devcontainer/scripts/install-deps.sh",
		"lint": "biome check .",
		"lint:boundaries": "biome lint src",
		"lint:fix": "biome check --write .",
		"prepare": "test -d .git && lefthook install || echo 'Skipping lefthook install (not a git repository)'",
		"setup": "bash .devcontainer/scripts/setup-dev.sh",
		"start": "next start",
		"start:all": "concurrently \"pnpm start\" \"pnpm worker:start\" --names \"app,worker\" --prefix-colors \"cyan,magenta\"",
		"test": "vitest",
		"test:unit": "vitest run tests/unit",
		"test:integration": "vitest run tests/integration",
		"test:e2e": "playwright test",
		"test:e2e:debug": "playwright test --debug",
		"test:e2e:ui": "playwright test --ui",
		"test:watch": "vitest --watch",
		"typecheck": "tsc --noEmit",
		"update-types": "pnpm dlx supabase gen types typescript --project-id hjdjtxzsuszjkrsdlxgl --schema public > src/types/database.ts",
		"verify-setup": "bash .devcontainer/scripts/verify-setup.sh"
	},
	"type": "module",
	"version": "0.1.0",
	"pnpm": {
		"overrides": {
			"import-in-the-middle": "1.15.0"
		}
	}
}
